subject "Test Subject 1"
chapterNumber 1.1
chapterName "Sandbox"

@multi_choice "Choose the **planets** which are in our solar system" {
    @options {
          "Sun",
          "Earth" correct,
          "Moon",
          "Neptune" correct,
          "Comets",
          "Asteroids",
          "Kepler",
          "Birds"
    }
    @numOptionsToShow 4
    @explanation "Earth and Neptune are planets, others are not."
}


@multi_choice "Given \\(a\\), \\(u\\), \\(t\\), choose the correct formulae
for finding \\(v\\)." {
    @options {
        "\\(v = u + at\\)" correct,
        "\\(v = \\frac 1 2 at^2\\)",
        "\\(v = \\frac S t\\)",
        "{{@img teeth.png}}"
    }
}